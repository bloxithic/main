local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local Options = Fluent.Options

local Window = Fluent:CreateWindow({
    Title = "notepad",
    SubTitle = "by Bloxithic & my friend",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.RightShift -- Used when theres no MinimizeKeybind
})

local Tabs = {
    Player = Window:AddTab({ Title = "Main", Icon = "house" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local TargetWalkspeed
local Speed = Tabs.Main:AddSlider("Speed", {
        Title = "Speed",
        Description = "",
        Default = 15,
        Min = 1,
        Max = 100,
        Rounding = 1,
        Callback = function(Value)
		TargetWalkspeed = Value
        end
})

local InstantInteract = Tabs.Main:AddToggle("Instant Interact", {Title = "Instant Interact", Default = false })

    Toggle:OnChanged(function()
        fireproximityprompt(prompt)
    end)

    Options.MyToggle:SetValue(false)

